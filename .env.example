# Qwen3-VL 推理服务环境变量配置

# HuggingFace 配置
HF_HOME=/models/.cache
HF_ENDPOINT=https://huggingface.co  # 如需使用镜像源，修改为 https://hf-mirror.com

# 模型配置
MODEL_PATH=/models/Qwen3-VL-32B-Instruct
TENSOR_PARALLEL_SIZE=4
MAX_MODEL_LEN=8192

# LMDeploy 配置
LMDEPLOY_PORT=23333
LMDEPLOY_EXECUTOR_BACKEND=mp
LMDEPLOY_SKIP_WARMUP=true
LMDEPLOY_ENABLE_CHECK_ENV=false

# 推理配置
MAX_TOKENS=2048
TEMPERATURE=0.7
TOP_P=0.9

# 服务端口
SERVICE_PORT=20000

# 日志级别
LOG_LEVEL=INFO
